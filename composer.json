{
    "name": "iporto/laravel-http-utils",
    "description": "Utilities for HTTP operations in Laravel, including conversion to curl commands",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ot√°vio Nogueira",
            "email": "otavio@iporto.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "IPorto\\HttpUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IPorto\\HttpUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IPorto\\HttpUtils\\HttpUtilsServiceProvider"
            ],
            "aliases": {
                "HttpUtils": "IPorto\\HttpUtils\\Facades\\HttpUtils"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}